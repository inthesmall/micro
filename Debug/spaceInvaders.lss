                                 	push r20
                                 	push r19
                                 	push r18
                                 	push r16
                                 	ldi r20, @0
                                 	mov r19, @1
                                 	rcall writeRegister
                                 	pop r16
                                 	pop r18
                                 	pop r19
                                 	pop r20
                                 .ENDMACRO
                                 ; WRITESTRING [0],[1] writes string [0] of length [1](reg)
