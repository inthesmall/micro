                                 ; REGISTER [0],[1] writes [1] to [0]
                                 .MACRO REGISTER
                                 	push r20
                                 	push r19
                                 	push r18
                                 	push r16
                                 	ldi r20, @0
                                 	ldi r19, @1
                                 	call writeRegister
                                 	pop r16
                                 	pop r18
                                 	pop r19
                                 	pop r20
                                 .ENDMACRO
